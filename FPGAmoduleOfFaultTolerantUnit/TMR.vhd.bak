----------------------------------------------------------------------
-- Блок TMR модуля FPGAmodule узла FaultTolerantUnit.
-- Функциональный элемент (вычислитель) резервирован двумя
-- аналогичными элементами.
-- Логика работы функционального элемента заключается в нахождении
-- контрольной суммы входной последовательности, рассчитанной
-- алгоритмом CRC32.
----------------------------------------------------------------------
-- Порождающий полином (31:0):
-- 1+x^1+x^2+x^4+x^5+x^7+x^8+x^10+x^11+x^12+x^16+x^22+x^23+x^26+x^32
----------------------------------------------------------------------
library ieee;
use ieee.std_logic_1164.all;

entity TMR is
    port (
        clk : in std_logic; -- тактовый сигнал
        data_in : in std_logic_vector(31 downto 0); -- входные данные (32-битная последовательность)
        CRC32_1, CRC32_2, CRC32_3 : inout std_logic_vector(31 downto 0); -- контрольная сумма входных данных, рассчитанная алгоритмом CRC32 (32-битная последовательность)
		  voting_element_in_1, voting_element_in_2, voting_element_in_3 : out std_logic_vector(31 downto 0); -- входные значения голосующего элемента
		  voting_element_out : out std_logic_vector(31 downto 0) -- выходное значение голосующего элемента
    );
end entity TMR;

architecture behavioral of TMR is
	signal LFSR_1, LFSR_2, LFSR_3 : std_logic_vector(31 downto 0) := (others => '0'); -- регистры сдвига с линейной обратной связью (LFSR)
